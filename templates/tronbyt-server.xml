<?xml version="1.0"?>
<Container version="2">
  <Name>tronbyt-server</Name>
  <Repository>ghcr.io/tronbyt/server:2</Repository>
  <Registry>https://ghcr.io/tronbyt/server</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/194050-support-bigsing-tronbyt-server/</Support>
  <Project>https://github.com/tronbyt</Project>
  <Overview>Tronbyt is an ecosystem replacement for Tidbyt. Tronbyt-server is the system for adding and configuring apps as well as serving to the Tronbyt hardware.</Overview>
  <Category>HomeAutomation: Productivity: Other:</Category>
  <WebUI>http://[IP]:[PORT:8000]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/bigsing/unraid-templates/refs/heads/main/images/tronbyt-server.ico</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Port" Target="8000" Default="8000" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">8000</Config>
  <Config Name="Timezone" Target="/etc/localtime" Default="/etc/localtime" Mode="ro" Description="used to sync docker with host time" Type="Path" Display="always" Required="true" Mask="false">/etc/localtime</Config>  
  <Config Name="Config Path 1" Target="/app/data" Default="/mnt/user/appdata/tronbyt/data" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false"></Config>
  <Config Name="Config Path 2" Target="/app/users" Default="/mnt/user/appdata/tronbyt/users" Mode="rw" Description="Can be removed after migrating from v1 to v2 and one time migration is complete." Type="Path" Display="always" Required="true" Mask="false"></Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">100</Config>
  <TailscaleStateDir/>
</Container>
